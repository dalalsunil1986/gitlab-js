{
  "name": "gitlab-js",
  "version": "0.1.0",
  "description": "gitlab JS interface",
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "npm test",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-coverage-report": "codecov"
  },
  "keywords": [
    "api",
    "cli",
    "gitlab",
    "js"
  ],
  "author": "rumax",
  "license": "MIT",
  "devDependencies": {
    "codecov": "^3.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^22.0.4"
  },
  "dependencies": {
    "extra-log": "^1.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "bug": "https://github.com/facebook/jest/issues/5082",
    "XcollectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
